{
  "name": "medaction",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "load:quick": "artillery run tests/load/quick-test.yml",
    "load:full": "artillery run tests/load/artillery.yml",
    "load:report": "artillery run tests/load/artillery.yml --output report.json && artillery report report.json",
    "clean": "rm -rf .next coverage test-results playwright-report .swc tsconfig.tsbuildinfo",
    "clean:all": "npm run clean && rm -rf node_modules",
    "fresh": "npm run clean:all && npm install",
    "analyze": "npx depcheck",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "seed:permissions": "tsx prisma/seed-permissions.ts",
    "docker:build": "docker build -t medaction:latest .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f app",
    "postinstall": "prisma generate",
    "i18n:extract": "tsx scripts/i18n/extract-keys.ts",
    "i18n:validate": "tsx scripts/i18n/validate-translations.ts",
    "i18n:sync": "tsx scripts/i18n/sync-locales.ts",
    "i18n:translate": "tsx scripts/i18n/translate-missing.ts"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.7.5",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-pg": "^5.22.0",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@types/leaflet": "^1.9.21",
    "@types/mapbox-gl": "^3.4.1",
    "@types/negotiator": "^0.6.4",
    "@types/qrcode": "^1.5.6",
    "archiver": "^7.0.1",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.25",
    "glob": "^13.0.0",
    "ioredis": "^5.8.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.556.0",
    "mapbox-gl": "^3.17.0",
    "negotiator": "^1.0.0",
    "next": "14.1.0",
    "next-auth": "^4.24.13",
    "next-intl": "3.26.0",
    "next-themes": "^0.4.6",
    "otplib": "^12.0.1",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.68.0",
    "react-leaflet": "^4.2.1",
    "recharts": "^3.5.1",
    "sonner": "^2.0.7",
    "swr": "^2.3.8",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.6",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/archiver": "^7.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/pg": "^8.15.6",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "axios": "^1.13.2",
    "chalk": "^5.6.2",
    "cheerio": "^1.1.2",
    "cli-table3": "^0.6.5",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.7",
    "eslint-config-prettier": "^10.1.8",
    "figlet": "^1.9.4",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "ora": "^9.0.0",
    "postcss": "^8",
    "prettier": "^3.7.3",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed-real-data.ts"
  }
}